<?xml version="1.0"?>
<TranslateStatus xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xsi:noNamespaceSchemaLocation="status-translator.xsd">
    <Status>
        <originalName>old</originalName>
        <translatedName>new</translatedName>
        <originalType>INT</originalType>
        <translatedType>HEALTH</translatedType>
        <default>BAD</default>
        <Maps>
            <Map>
                <from>0</from>
                <to>GOOD</to>
            </Map>
            <Map>
                <from>1</from>
                <to>WARNING</to>
            </Map>
            <Map>
                <from>2</from>
                <to>BAD</to>
            </Map>
        </Maps>
    </Status>
</TranslateStatus>